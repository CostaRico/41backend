.entity.col-md-6.btn-group-vertical{ class: "action-for-object-#{object.id}" }
  = link_to t('.edit'), [:edit_adminka, object], role: 'button',
  class: "btn btn-warning approve-button-for-object-#{object.id}"

  = link_to t('.delete'), [:adminka, object], method: :delete, role: 'button',
  class: "btn btn-danger approve-button-for-object-#{object.id}"
